library(shiny)
ui <- fluidPage(
titlePanel(h2("School District Expenditures per Student")),
sidebarLayout(
sidebarPanel(
selectInput(inputId = "schooldistrict",
label = "Choose a School District",
choices = c(afr_expenditures_all$School.District),
